        <ul class="list-sidebar bg-defoult">
          <li>
            <a href="/admin" data-toggle="collapse" data-target="#tongquan">
              <i class="fa fa-th-large"></i>
              <span class="nav-label"> Tổng quan</span>
            </a>
          </li>

          <li>
            <a href="/admin/warehouse" data-toggle="collapse" data-target="#khohang" class="active">
              <i class="fa fa-th-large"></i>
